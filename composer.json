{
  "name": "omaicobuss/yii2-user-module",
  "description": "Módulo Yii2 para gerenciamento de usuários reutilizável.",
  "type": "yii2-extension",
  "require": {
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "~2.0.2"
  },
  "autoload": {
    "psr-4": {
  "omaicobuss\\user\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Omaico Buss",
      "email": "omaicobuss@gmail.com",
      "homepage": "https://github.com/omaicobuss"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  }
}
