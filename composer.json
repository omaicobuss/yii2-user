{
  "name": "acme/yii2-user-module",
  "description": "Módulo Yii2 para gerenciamento de usuários reutilizável.",
  "type": "library",
  "require": {
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-bootstrap5": "^2.0",
    "yidas/yii2-bower-asset": "^2.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "app\\modules\\user\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
